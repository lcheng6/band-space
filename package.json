  "devDependencies": {
    "react-scripts": "1.0.10"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/styles/sass -o src/styles/css",
    "watch-css": "npm run build-css && node-sass-chokidar src/styles/sass -o src/src/styles/css --watch --recursive",
    "build-js": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "start-js": "react-scripts start",
    "start-client": "npm-run-all -p watch-css start-js",
    "start-server": "nodemon server/server.js",
    "start": "npm-run-all --parallel start-server start-client",
    "build": "npm run build-css && react-scripts build"
  },
  "proxy": "http://localhost:3001"
}
