    "react-dom": "^15.6.1",
    "react-google-maps": "^7.1.0",
    "react-infinite": "^0.11.0",
    "react-inlinesvg": "^0.6.2",
    "react-motion": "^0.5.0",
    "react-redux": "^4.4.8",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^4.0.8",
    "react-svg-inline": "^2.0.0",
